# Можно указать один ключ (например, ProxyAPI), он будет использован для чата
VITE_PROXYAPI_API_KEY=sk-xxxx

# (опционально) если нужен отдельный ключ для чата
VITE_OPENAI_API_KEY=sk-xxxx

# (опционально) по умолчанию: https://openai.api.proxyapi.ru/v1/chat/completions
VITE_OPENAI_API_URL=https://openai.api.proxyapi.ru/v1/chat/completions

# Основная модель для ответов
VITE_LLM_MODEL_MAIN=gpt-3.5-turbo

# (опционально) если не указана, используется VITE_LLM_MODEL_MAIN
VITE_LLM_MODEL_SUMMARY=

# Настройки сжатия истории
VITE_SUMMARY_CHUNK_SIZE=5
VITE_SUMMARY_KEEP_LAST=5
VITE_SUMMARY_ENABLED_DEFAULT=true
VITE_SUMMARY_LANGUAGE=ru

VITE_BASE_PATH=https://github.com/mvk85/mvk85.github.io.git
